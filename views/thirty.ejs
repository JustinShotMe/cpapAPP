<%- include('partials/headAndSide') %>

<div class="col d-flex flex-column h-sm-100 mainSection mainSectionDark">
  <main class="row overflow-auto">
    <div class="col pt-4">
      <h1 class="text-light text-center pageHeader">Fourteen Day Dashboard</h1>

      <div class="d-flex flex-wrap flex-row justify-content-center mb-5">
        <h3 class="text-light" id="startDate">Dash </h3>
        <h3 class="text-light"> &nbsp; - &nbsp; </h3>
        <h3 class="text-light" id="endDate"> Board</h3>
      </div>

      <div class="d-flex flex-wrap flex-row justify-content-around chartRow">

        <div class="col-sm-10 col-lg-5 mb-5">
          <h2 class="text-light text-center">Duration</h2> <canvas id="fourteenDuration"></canvas>
        </div>

        <div class="col-sm-10 col-lg-5 mb-5">
          <h2 class="text-light text-center">Days Missed</h2> <canvas id="fourteenUsed"></canvas>
        </div>

      </div>

      <h2 class="text-light text-center">Usage Data</h2>
      <div class="container col-lg-10 table-responsive-lg">
        <!-- <div class="container col-9 table-responsive-md"> -->
        <table class="table table-hover table-sm table-striped table-bordered mb-5">
          <!-- <table class="table table-hover table-bordered mb-5"> -->
          <thead class="">
            <tr>
              <th class="text-light text-center" scope="col">Date</th>
              <th class="text-light text-center" scope="col">Hours</th>
              <th class="text-light text-center" scope="col">AHI</th>
              <th class="text-light text-center" scope="col">Breaths</th>
              <th class="text-light text-center" scope="col">Pressure</th>
            </tr>
          </thead>
          <tbody>
          </tbody>
        </table>
      </div>



    </div>
  </main>



  <script src="/js/thirty.js"></script>
  <%- include('partials/footer') %>